	<munit:config name="hellomule-test-suite.xml" />
	<munit:test name="hellomule-test-suite-hellomuleFlowTest" doc:id="b8358be8-1cab-476d-8543-dc445c117ae6" description="Test">
		<munit:execution >
			<munit:set-event doc:name="Set Event" doc:id="e48d7982-d5b5-4465-a005-bc586bda0139">
				<munit:attributes value='#[{queryParams: {firstName:"Namrata",lastName:"Bawane"}}]' />
			</munit:set-event>
			<flow-ref doc:name="Flow-ref to hellomuleFlow" doc:id="020ec412-8bfa-4d3b-98a6-28eab40cbb10" name="hellomuleFlow"/>
		</munit:execution>
		<munit:validation >
			<logger level="INFO" doc:name="Logger" doc:id="0692c956-d1e5-4159-87ff-8adef67339a3" message="Validation Block"/>
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="b171b6b7-3d14-408f-9e7c-f053784c5138" actual='#[payload.message]' expected='Welcome to mule Namrata Bawane' message="Response message is incorrect"/>
		</munit:validation>
	</munit:test>
	<munit:test name="hellomule-success-Test" doc:id="a24a2fbf-008b-41b6-a46d-79cfa97a832b" >
		<munit:execution>
			<munit:set-event doc:name="Set Event" doc:id="0da237df-c1dd-4c71-851b-ef679ed638b3">
				<munit:attributes value='#[{queryParams: {firstName:"Namrata",lastName:"Bawane"}}]' />
			</munit:set-event>
			<flow-ref doc:name="Flow-ref to hellomuleFlow" doc:id="c9e50efd-6f1e-461b-9d45-20bf576c86dc" name="hellomuleFlow" />
		</munit:execution>
		<munit:validation >
			<logger level="INFO" doc:name="Logger" doc:id="440bc76d-4ba1-47bb-878c-377c2966d750" message="Validation Block" />
			<munit-tools:assert-that doc:name="Assert that" doc:id="464d6f80-2afc-40bb-82a4-f467b154251a" is='#[MunitTools::equalTo("Welcome to mule Namrata Bawane")]' expression="#[payload.message]"/>
		</munit:validation>
	</munit:test>
