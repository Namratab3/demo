		<munit:execution >
			<munit:set-event doc:name="Set Event" doc:id="a0149953-6afa-4210-b0e8-cc732e4a0b6e" >
				<munit:attributes value='#[{queryParams: {firstName:"Namrata",lastName:"Bawane"}}]' />
			</munit:set-event>
			<flow-ref doc:name="Flow-ref to hellomuleFlow" doc:id="2220f2c8-699c-44cc-8495-64a114b8bc27" name="hellomuleFlow" />
		</munit:execution>
		<munit:validation >
			<logger level="INFO" doc:name="Logger" doc:id="e896cde0-ac0f-4de6-8ca2-429a0c2c35db" message="Validation Block" />
			<munit-tools:assert doc:name="Assert expression" doc:id="2e2a1734-9a89-44f7-943b-02092f6de400" >
				<munit-tools:that ><![CDATA[#[import * from dw::test::Asserts 
 ---
payload must equalTo({
	"message": "Welcome to mule Namrata Bawane"
})]]]></munit-tools:that>
			</munit-tools:assert>
		</munit:validation>
	</munit:test>
